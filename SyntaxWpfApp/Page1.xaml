<Page x:Class="SyntaxWpfApp.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SyntaxWpfApp"
      
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page1">

    <Grid>
        <Button x:Name="BtnGo" Content="Go to Page2" Width="100" Height="30" Click="BtnGo_Click"/>
        <Label xmlns:logic="clr-namespace:BusinessLogic" >
            <logic:Car x:Name="Car2" Speed="100" Color="White">
                <logic:Car.Driver>
                    <logic:Human FirstName="Hugo" HasLicense="True"/>
                </logic:Car.Driver>
            </logic:Car>
        </Label>
    </Grid>
</Page>
